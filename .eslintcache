quired\n          >\n          </FormInput>\n          <FormInput\n            type='email'\n            name='email'\n            value={email}\n            onChange={this.handleChange}\n            label='Email'\n            required\n          >\n          </FormInput>\n          <FormInput\n            type='password'\n            name='password'\n            value={password}\n            onChange={this.handleChange}\n            label='Password'\n            required\n          >\n          </FormInput>\n          <FormInput\n            type='password'\n            name='confirmPassword'\n            value={confirmPassword}\n            onChange={this.handleChange}\n            label='Confirm Password'\n            required\n          >\n          </FormInput>\n          <CustomButton type='submit'>SIGN UP</CustomButton>\n        </form>\n      </div>\n    )\n  }\n}\n\nexport default SignUp;","/Users/vgane00/learning/crwn-clothing/src/redux/store.js",[],"/Users/vgane00/learning/crwn-clothing/src/redux/user/user.reducer.jsx",["108"],"/Users/vgane00/learning/crwn-clothing/src/redux/user/user.actions.js",[],"/Users/vgane00/learning/crwn-clothing/src/components/cart-icon/cart-icon.component.jsx",[],"/Users/vgane00/learning/crwn-clothing/src/components/cart-dropdown/cart-dropdown.component.jsx",[],"/Users/vgane00/learning/crwn-clothing/src/redux/cart/cart.types.js",[],"/Users/vgane00/learning/crwn-clothing/src/redux/cart/cart.reducer.js",[],"/Users/vgane00/learning/crwn-clothing/src/redux/cart/cart.actions.js",[],"/Users/vgane00/learning/crwn-clothing/src/redux/cart/cart.utils.js",["109","110"],"/Users/vgane00/learning/crwn-clothing/src/components/cart-item/cart-item.component.jsx",[],{"ruleId":"111","severity":1,"message":"112","line":21,"column":18,"nodeType":"113","messageId":"114","endLine":21,"endColumn":20},{"ruleId":"115","severity":1,"message":"116","line":1,"column":10,"nodeType":"117","messageId":"118","endLine":1,"endColumn":16},{"ruleId":"111","severity